  "mainALGO": "<?php\n\n$list = array(2, 4, 6, 7, 3, 1, 5);\n$val1 = 4;\n$val2 = 9;\n\nfunction linear_search($list, $val, $count)\n{\n    for($i = 0; $i < $count ; $i++)\n    {\n        if($list[$i] == $val)\n            return $i;\n    }\n\n    return -1;\n}\n\nif(linear_search($list,$val1,count($list)) != -1)\n        echo $val1.\" Found<br>\" ;\n    else\n        echo $val1.\" Not Found<br>\";\n\nif(linear_search($list,$val2,count($list)) != -1)\n        echo $val2.\" Found<br>\" ;\n    else\n        echo $val2.\" Not Found<br>\";\n\n?>\n"
